#include<stdio.h>
#include<stdlib.h>
void main()
{
    int a[100],k,n;
    printf("Enter the no. of elements:");
    scanf("%d",&n);
    printf("elements:\n");
    for(k=0;k<n;k++)
    scanf("%d",&a[k]);
    }
    void bubble(int a[],int n)
    {          
         for(int i=0;i<n-1;i++)
         {
             for(int j=0;j<n-i-1;j++)
             {
                 if(a[j]>a[j+1])
                 {
                     int temp=a[j];
                     a[j]=a[j+1];
                     a[j+1]=temp;
                     
                 }
             }
         } 
    }
        
